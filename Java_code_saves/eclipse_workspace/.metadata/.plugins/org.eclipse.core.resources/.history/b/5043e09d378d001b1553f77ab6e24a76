class Employee{
	
	String name;
	float base_money;
	float increse;
	int num;
	
	public Employee(String name,int num)
	{
		this.name=name;
		this.num=num;
		
	}
	float count_sum( float base_money,float increse)
	{
		
		return  base_money*(1+increse);
	}

	
	
	
}
